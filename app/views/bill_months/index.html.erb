<p id="notice"><%= notice %></p>

<h1>Bill Months</h1>

<table>
  <thead>
    <tr>
      <th>Month</th>
      <th>Status</th>
      <th>House</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bill_months.each do |bill_month| %>
      <tr>
        <td><%= bill_month.month %></td>
        <td><%= bill_month.status %></td>
        <td><%= bill_month.house %></td>
        <td><%= link_to 'Show', bill_month %></td>
        <td><%= link_to 'Edit', edit_bill_month_path(bill_month) %></td>
        <td><%= link_to 'Destroy', bill_month, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bill Month', new_bill_month_path %>
